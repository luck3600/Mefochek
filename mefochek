lst = ["–±–∞—Ä–∞—à–µ–∫","üí© ","–ø–µ—Ä—Å–∏–∫","—Ö–æ–ª–æ–¥–∏–ª—å–Ω–∏–∫","—Ç–æ—Å—Ç–µ—Ä","T-34 ‚Äî —Å–æ–≤–µ—Ç—Å–∫–∏–π —Å—Ä–µ–¥–Ω–∏–π —Ç–∞–Ω–∫ –ø–µ—Ä–∏–æ–¥–∞ –í–µ–ª–∏–∫–æ–π –û—Ç–µ—á–µ—Å—Ç–≤–µ–Ω–Ω–æ–π –≤–æ–π–Ω—ã, –≤—ã–ø—É—Å–∫–∞–ª—Å—è —Å–µ—Ä–∏–π–Ω–æ —Å 1940 –≥–æ–¥–∞. –í —Ç–µ—á–µ–Ω–∏–µ 1942‚Äî1947 –≥–æ–¥–æ–≤ ‚Äî –æ—Å–Ω–æ–≤–Ω–æ–π —Ç–∞–Ω–∫ –†–ö–ö–ê –∏ –í–° –°–°–°–† –¥–æ –ø–µ—Ä–≤–æ–π –ø–æ–ª–æ–≤–∏–Ω—ã 1944 –≥–æ–¥–∞, –¥–æ –ø–æ—Å—Ç—É–ø–ª–µ–Ω–∏—è –≤ –≤–æ–π—Å–∫–∞ –µ–≥–æ –º–æ–¥–∏—Ñ–∏–∫–∞—Ü–∏–∏ –¢-34-85."]

print("–£ –Ω–∞—Å –≤ –º–µ—â–æ—á–∫–µ:")
for i in lst:
    print(i)
    
print("–í—Å–µ–≥–æ –≤ –º–µ—â–æ—á–∫–µ:",len(lst))

idx = int(input("–í—ã–±–µ—Ä–∏ —á—Ç–æ —Ö–æ—á–µ—à—å –¥–æ—Å—Ç–∞—Ç—å –∏–∑  –º–µ—â–æ—á–∫–∞"))
print("–ø–æ–¥ –Ω–æ–º–µ—Ä–æ–º",idx,"–Ω–∞—Ö–æ–¥–∏—Ç—Å—è",lst[idx])

print('–í–≤–µ–¥–∏—Ç–µ –Ω–∞—á–∞–ª—å–Ω—ã–π –∏ –∫–æ–Ω–µ—á–Ω—ã–π –∏–Ω–¥–µ–∫—Å –¥–ª—è —Å—Ä–µ–∑–∞ –º–µ—â–æ—á–∫–∞')
i_start = int(input("–ù–∞—á–∞–ª—å–Ω—ã–π –∏–Ω–¥–µ–∫—Å"))
i_end = int(input("–ö–æ–Ω–µ—á–Ω—ã–π –∏–Ω–¥–µ–∫—Å: "))
print("–í —Å—Ä–µ–∑–µ —Å—É–º–∫–∏", i_start, ":", i_end, "–Ω–∞—Ö–æ–¥–∏—Ç—Å—è: ")
for i in lst[i_start:i_end]:
    print(i)
    
    
print("–í—ã –Ω–∞—à–ª–∏ –º–µ—â–æ—á–µ–∫ –∏ —Ç–∞–º –±—ã–ª–æ:")
dropbox = ["–†–∞–∫—É—à–∫–∞","23 –ø—É—á–∫–∞ —É–∫—Ä–æ–ø–∞" , "25 –∫–∞—Ä—Ç–æ—à–µ–∫", "17 –º–∞–Ω–¥–æ–≤–æ—à–µ–∫"]
for i in dropbox:
    print(i)
input("–ù–∞–∂–º–∏—Ç–µ Enter —á—Ç–æ–±—ã –¥–æ–±–∞–≤–∏—Ç—å —ç—Ç–∏ –≤–µ—â–∏ –≤ —Å–≤–æ–π –º–µ—â–æ—á–µ–∫")
lst += dropbox
print("–¢–µ–ø–µ—Ä—å –≤ –Ω–∞—à–µ–º –º–µ—â–æ—á–∫–µ –ª–µ–∂–∏—Ç:")
for i in lst:
    print(i)
    
print("–í–∞–º –≤—ã–ø–∞–ª —à–∞–Ω—Å –∑–∞–º–µ–Ω–∏—Ç—å –æ–¥–∏–Ω –∏–∑ –ø—Ä–µ–¥–º–µ—Ç–æ–≤ –≤ –≤–∞—à–µ–º –º–µ—â–æ—á–∫–µ")
n_thing = input("–í–≤–µ–¥–∏—Ç–µ –ø—Ä–µ–¥–º–µ—Ç: ")
idx_n = int(input("–í–≤–µ–¥–∏—Ç–µ –∏–Ω–¥–µ–∫—Å: "))
lst[idx_n] = n_thing
print("—Ç–µ–ø–µ—Ä—å –≤ –Ω–∞—à–µ–º –º–µ—â–æ—á–∫–µ –ª–µ–∂–∏—Ç:")
for i in lst:
    print(i)
    
print("–ë—É! –ò—Å–ø—É–≥–∞–ª—Å—è , –±–æ–π—Å—è –Ω–∞ —Ç–µ–±—è –≤–æ—Ä–∏—à–∫–∞ –Ω–∞–ø–∞–ª")
del lst[0:4]
print("—Ç–µ–ø–µ—Ä—å –≤ –Ω–∞—à–µ–º —Å—É–º–∫–µ –ª–µ–∂–∏—Ç:")
for i in lst:
    print(i)
